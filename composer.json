{
    "name": "fluxbb/markdown",
    "description": "An extensible markdown parser for PHP 5.4+",
    "keywords": ["markdown", "commonmark", "parser"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kazuyuki Hayashi",
            "email": "hayashi@valnur.net"
        },
        {
            "name": "Franz Liedke",
            "email": "franz@fluxbb.org"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "FluxBB\\Markdown\\": "src"
        }
    },
    "require": {
        "php": ">5.4.0",
        "symfony/options-resolver": "~2.3",
        "symfony/console": "~2.3"
    },
    "require-dev": {
        "symfony/finder": ">=2.3,<2.5-dev",
        "symfony/stopwatch": ">=2.3,<2.5-dev",
        "kzykhys/pygments": ">=1.0"
    },
    "suggest": {
        "kzykhys/pygments": ">=1.0"
    },
    "bin": ["bin/markdown"],
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
